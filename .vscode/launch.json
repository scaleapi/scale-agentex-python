{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to AgentEx Worker",
            "type": "debugpy",
            "request": "attach",
            "connect": {
                "host": "localhost",
                "port": 5678 // change this if your debug server is running on a different port
            },
            "pathMappings": [
                {
                    "localRoot": "${workspaceFolder}",
                    "remoteRoot": "."
                }
            ],
            "justMyCode": false,
            "console": "integratedTerminal"
        }
    ]
} 